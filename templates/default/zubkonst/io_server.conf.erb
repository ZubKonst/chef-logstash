input {
  tcp {
    host => localhost
    port => 5229
    codec => json_lines
  }
}

filter {
  unique {
    fields => [ "post_instagram_id", "post_created_time" ]
  }
  date {
    match => [ "post_created_time", "UNIX" ]
    target => "@timestamp"
  }
}

output {
  elasticsearch {
    host => localhost
    cluster => InstaGeo
  }
}
